@page
@model webclinic.Pages.AdminAnalyticsModel
@{
	ViewData["Title"] = "Admin Analytics";
	Layout = ".\\Shared\\_LayoutLogin.cshtml";
}


<div class="chart-container" width="600" height="400">
	<canvas id="barChart"></canvas>
</div>

<script type="module">
	document.addEventListener('DOMContentLoaded', (event) => {

		var ctx = document.getElementById('barChart');
		var myChart = new Chart(ctx, @Html.Raw(Model.ChartJson) );
	});
</script>
